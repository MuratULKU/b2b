@using Microsoft.AspNetCore.Components.Web.Virtualization

@typeparam TItem
@if(IsEnabled)
{
    <Virtualize Items="@Items" Context="item">
        @if(ChildContent != null)
        {
            @ChildContent(item);
        }
    </Virtualize>
}
else
{
    @foreach (var item in Items ?? Array.Empty<TItem>())
    {
        @ChildContent(item);
      
    }
}


